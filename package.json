{
  "name": "send_app",
  "version": "0.0.0",
  "private": true,
  "devDependencies": {
    "@citizenfx/client": "^2.0.7061-1",
    "@citizenfx/server": "^2.0.7061-1",
    "@originjs/vite-plugin-federation": "^1.3.2",
    "@types/node": "^14.17.34",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.14",
    "@types/react-router-dom": "^5.3.2",
    "@vitejs/plugin-react": "^4.1.1",
    "add": "^2.0.6",
    "autoprefixer": "^10.4.17",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "postcss": "^8.4.33",
    "react-refresh": "^0.14.0",
    "react-refresh-typescript": "^2.0.3",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^4.5.0"
  },
  "scripts": {
    "dev": "vite --mode dev",
    "preview": "vite preview --port 4173 --strictPort",
    "dev:browser": "concurrently \"build:watch --mode dev\" \"preview:dev\"",
    "dev:server": "tsx watch packages/server/server.ts",
    "game": "yarn build && yarn bundle && yarn move",
    "bundle": "sh scripts/bundle.sh",
    "move": "sh scripts/move.sh",
    "build:dev": "vite build --mode dev",
    "build:watch": "vite build --watch",
    "build:game": "vite build --mode game",
    "build": "pnpm build:client && pnpm build:server && vite build --mode game",
    "preview:dev": "vite preview --mode dev",
    "clean": "rm -rf dist",
    "watch": "concurrently \"yarn watch:client\" \"yarn watch:server\"  \"yarn watch:nui\"",
    "watch:nui": "esbuild src/index.tsx --outdir=dist/html --watch --target=es6 --bundle --loader:.png=dataurl",
    "watch:client": "esbuild packages/client/client.ts --bundle --watch --outfile=dist/client.js",
    "watch:server": "esbuild packages/server/server.ts --bundle --platform=node --watch --outfile=dist/server.js",
    "build:client": "esbuild packages/client/client.ts --bundle --outfile=dist/client.js",
    "build:server": "esbuild packages/server/server.ts --bundle --outfile=dist/server.js --platform=node"
  },
  "dependencies": {
    "@garmingo/framework-js-server": "^1.1.2",
    "esbuild": "^0.19.5",
    "eslint": "^8.10.0",
    "fivem-router": "^1.0.12",
    "i18next": "^23.6.0",
    "prettier": "^3.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-feather": "^2.0.10",
    "react-hot-loader": "^4.13.0",
    "react-query": "^3.39.3",
    "react-router-dom": "^5.3.0",
    "tsx": "^4.7.0",
    "vite-plugin-top-level-await": "^1.3.1",
    "zod": "^3.22.4"
  }
}
