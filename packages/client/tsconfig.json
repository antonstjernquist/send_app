{
  "compilerOptions": {
    "types": ["@citizenfx/client", "@types/node"],
    "moduleResolution": "Node16",
    "resolveJsonModule": true,
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "module": "Node16",
    "target": "es6",
    "allowJs": true,
    "lib": ["es2017"]
  },
  "include": ["./*.ts", "../shared"],
}
